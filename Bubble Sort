#include <iostream>
#include <bits/stdc++.h>
#include <math.h>
#include<vector> 
using namespace std;



vector<int> selection_sort(vector<int> arr )  
{
	for(int i =0 ; i< arr.size() ; i++)
	{
		for(int j = 0 ; j <arr.size()-i-1; j++)
		{
			if(arr[j]>arr[j+1])
				swap(arr[j],arr[j+1]);
		}

	}
	
	return arr;
}

int main() {
  vector<int> arr = {4,9,1,3,2};
  arr = selection_sort(arr);
  for(auto i : arr)
	cout<<i<<" ";
}
