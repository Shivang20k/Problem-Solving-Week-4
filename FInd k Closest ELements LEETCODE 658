
int findClosestElemnets(vector<int>& arr, int k, int x){
  //2 pointer approach
  sort(nums.begin(), nums.end())
  int start = 0 , end = arr.size()-1;
  while(end-start >= k)
  {
    if( x-arr[start] > arr[high]-x)
      l++;
    else
      h--;
  }
  return vector<int>(arr.begin()+start,  arr.end()+end+1);
}
